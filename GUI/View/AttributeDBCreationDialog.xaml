<v:DialogUserControl x:Class="GUI.View.AttributeDBCreationDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:local ="clr-namespace:GUI"
             xmlns:v="clr-namespace:GUI.View"
             xmlns:vm="clr-namespace:GUI.ViewModel"
             d:DesignHeight="280" d:DesignWidth="450">
    <!--<UserControl.DataContext>
        <vm:AttriDBCreationVM></vm:AttriDBCreationVM>
    </UserControl.DataContext>-->
    <Grid Background="White" >
        <GroupBox Margin="5" Header="属性库建库参数设置" FontSize="12">
            <StackPanel Grid.Row="0" Orientation="Vertical" HorizontalAlignment="Left" Margin="20">
                <CheckBox x:Name="isContinueWithError"  Margin="10" Content="允许出错后继续"
                          IsChecked="{Binding IsContinueWithError, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                <CheckBox x:Name="isInitDictionary" Margin="10" Content="初始化字典"
                           IsChecked="{Binding IsInitDictionary, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
            </StackPanel>
        </GroupBox>
        
    </Grid>
</v:DialogUserControl>
